{
  "version": 2,
  "name": "storia",
  "buildCommand": "npm run build:web",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index-web.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://storia-production.up.railway.app"
  }
}
